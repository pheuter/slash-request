{
  "manifest_version": 2,
  "name": "Slash Request",
  "version": "1.0",

  "description": "Analyze /request data",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "minimum_chrome_version": "10.0",
  "devtools_page": "devtools.html"
}